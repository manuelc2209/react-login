{
    "name": "react-ui",
    "website": "https://react-ui.manuelcorreia.org/",
    "version": "1.0.0",
    "private": false,
    "author": "Manuel Correia",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/manuelc2209/react-ui",
        "type": "git"
    },
    "dependencies": {
        "axios": "^1.6.7",
        "babel-loader": "8.2.5",
        "color": "4.2.0",
        "dayjs": "^1.11.7",
        "nanoid": "^4.0.1",
        "react": "18.2.0",
        "react-router-dom": "6.8.1",
        "react-scripts": "5.0.1",
        "react-select": "5.7.0",
        "react-syntax-highlighter": "^15.5.0",
        "react-test-renderer": "18.2.0",
        "storybook": "6.5.12",
        "styled-components": "5.3.6",
        "typescript": "4.8.4",
        "validate-color": "2.2.1"
    },
    "scripts": {
        "postinstall": "husky install",
        "start": "cross-env NODE_ENV=development && react-scripts start",
        "build": "cross-env NODE_ENV=production && react-scripts build",
        "test": "jest --coverage --runInBand",
        "test:nowatch": "jest --coverage --runInBand --watchAll=false",
        "eject": "react-scripts eject",
        "storybook": "start-storybook -p 6006 -s public",
        "build-storybook": "build-storybook -s public"
    },
    "eslintConfig": {
        "extends": "react-app",
        "overrides": [
            {
                "files": [
                    "**/*.stories.*"
                ],
                "rules": {
                    "import/no-anonymous-default-export": "off"
                }
            }
        ]
    },
    "jest": {
        "testEnvironment": "jsdom",
        "moduleNameMapper": {
            "\\.(css|less|scss|sass)$": "identity-obj-proxy"
        }
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@babel/plugin-proposal-export-default-from": "^7.16.7",
        "@babel/plugin-syntax-typescript": "7.18.6",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "@babel/plugin-transform-typescript": "^7.16.8",
        "@faker-js/faker": "^8.0.2",
        "@reduxjs/toolkit": "^1.8.6",
        "@shopify/eslint-plugin": "42.0.1",
        "@storybook/addon-actions": "^6.4.18",
        "@storybook/addon-essentials": "^7.6.17",
        "@storybook/addon-links": "^6.4.18",
        "@storybook/builder-webpack5": "^6.4.19",
        "@storybook/manager-webpack5": "^6.4.19",
        "@storybook/node-logger": "^7.6.17",
        "@storybook/preset-create-react-app": "^4.0.0",
        "@storybook/react": "6.5.12",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/color": "^3.0.3",
        "@types/jest": "29.2.0",
        "@types/node": "20.11.19",
        "@types/react": "18.0.21",
        "@types/react-dom": "^18.0.6",
        "@types/react-syntax-highlighter": "^15.5.11",
        "@types/react-test-renderer": "^17.0.1",
        "@types/styled-components": "5.1.26",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.0.0",
        "cross-env": "7.0.3",
        "eslint": "8.56.0",
        "husky": "^7.0.4",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.2.1",
        "jest-environment-jsdom": "^29.4.2",
        "react-dom": "^18.2.0",
        "ts-jest": "^29.0.3",
        "webpack": "^5.90.3"
    }
}
